{
    "name": "laradic/icon-generator",
    "description": "Generate FontAwesome, Foundation Icons or any custom icon font icons to multiple size and color image files like .ico/.png",
    "keywords": ["laradic", "icon", "icons", "generator", "fontawesome", "font-awesome", "foundation", "foundation-icons", "laravel"],
    "license": "MIT",
    "homepage": "https://la.radic.nl",
    "authors": [
        {"name": "Laradic", "homepage": "https://laradic.nl", "email": "info@laradic.nl"},
        {"name": "Robin Radic", "homepage": "http://robin.radic.nl", "email": "robin@laradic.nl", "role": "Developer"}
    ],
    "require": {
        "php": ">=7.1.0",
        "laradic/filesystem": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Laradic\\IconGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laradic\\Tests\\IconGenerator\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}
